page{width: 100%; height: 100%;}
.city_listCont{background: #f5f3f2;height: 100%;}
.list_title{width: 100%; height: 30rpx;background:#eb5033; color: #fff;font-size: 22rpx; line-height: 30rpx;padding-left:20rpx;}
.list_position{height: 90rpx; line-height: 90rpx; font-size: 24rpx;color: #eb5033; text-align: center;}
.list_position .now{color: #666666; padding-right: 22rpx;} 
.list_position image{width: 36rpx; height: 36rpx; padding: 0 18rpx 0 70rpx;}
.lists{width: 100%; height: 85%;}
.lists .lists_left{width: 50%; height: 85%;}
/* 隐藏滚动条 */
::-webkit-scrollbar  
{  
    width: 0;  
    height: 0; 
    color: transparent;  
}  
.lists .lists_left .lists_lefts{height: 78rpx; border-bottom: 1px solid #dedede; padding:0 20rpx;  text-align: center; color: #333333; font-size: 30rpx;  line-height: 78rpx;} 
.lists .lists_left .active{background: #fff; border-bottom: none;}
.actives{background: #f5f3f2;border-bottom: none; }
.bar{width: 100%; height: 10rpx; background: #dedede;}
